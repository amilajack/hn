{
  "name": "simple-hn",
  "version": "0.0.1",
  "description": "Cross-Platform Hacker News Client",
  "homepage": "http://github.com/premii/hn",
  "repository": "git://github.com/premii/hn.git",
  "author": "DHARMESH PATEL <DHARMESH@PREMII.COM>",
  "main": "index.js",
  "keywords": [
    "hacker news",
    "simple",
    "client"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --bail --timeout 5000 --inline-diffs --async-only --growl --compilers js:babel-register --recursive --require ./test/setup.js test/*.spec.js",
    "test-all": "cross-env NODE_ENV=test npm run lint && npm run test && npm run build",
    "build": "cross-env NODE_ENV=production babel app/**/* --out-dir dist && npm run build-web",
    "build-web": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.js --progress --profile --colors --display-error-details",
    "styles": "postcss --use autoprefixer ./dist/main.css -d ./dist/main.css",
    "clean": "rm -rf dist",
    "lint": "cross-env NODE_ENV=test eslint app/**/* test *.js",
    "serve": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack",
    "version": "npm run build",
    "hot-server": "node -r babel-register server.js",
    "dev": "concurrently --kill-others \"npm run hot-server\""
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.5.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "favicons": "^4.7.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "react-hot-loader": "^1.3.0",
    "uglifyify": "^3.0.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "ionicons": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.4.2",
    "moment": "^2.13.0",
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.0",
    "zepto": "github:madrobby/zepto"
  },
  "engines": {
    "node": "4.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "devEngines": {
    "node": "4.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
